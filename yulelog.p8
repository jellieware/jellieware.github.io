 7
}

ptrns={
 0,
 0b1000100010001000,
 0b1010100010101000,
 0b1111000011110000,
 0b1010101010101010,
 0b1000101000101010,
 0b1100110000110011,
 0b1010010110100101,
 0b1010000010100000,
 0b1010101001010101
}
ptrn=8

function _init()
 cls()
 str="l o a d i n g . . ."
 print(str,64-#str*2-2,61,7)
 flip()

 init_log()
 init_circle_draw()

 cls() flip()
 
 for c=0,15 do
  --pal(c,plt[flr(c*#plt/16)+1],1)
  pal(c,plt[c+1],1)
 end
 
 t=0
 
 balls={}
 sparks={}
 
 for i=0,31 do
  create_ball(56+rnd(16),108)
  foreach(balls,update_ball)
 end
 
 for i=0,7 do
  create_spark(56+rnd(16),100)
--  foreach(sparks,update_spark)
 end

 slog=0
 
 message=true

 palt(15,true)
 palt(0,false)
 
 _draw()
 flip()
 extcmd("rec")
end

function _update()
 t+=0.01
 
 if t>=2 and t<2.01 then
  extcmd("rec")
 end
 
 if t>=20 and t<20.01 then
  extcmd("video")
 end
 
 if stat(1)>=1 then
  cls(2)
  while true do
   flip()
  end
 end
 
 if t%0.03<0.01 then
  slog+=sgn(flr(rnd(2))-0.5)
  slog=mid(slog,0,2)
 end

 --for i=0,1 do
 local y=108
 if rnd(1.3)>1 then
  create_ball(40+rnd(48),y-8)
 end
 create_ball(40+rnd(48),y-8)
 create_ball(48+rnd(32),y)
 --end
 
 if rnd(24)<1 then
  create_spark(48+rnd(32),y-8)
 end
 
 foreach(balls,update_ball)
 foreach(sparks,update_spark)
 
 if btnp(2) then
  ptrn=(ptrn-2)%#ptrns+1
 end
 if btnp(3) then
  ptrn=ptrn%#ptrns+1
 end
 
 if btnp(4) then
  message=not message
 end
end

function _draw()
-- cls()

 fillp(ptrns[ptrn])

 local c=8*16+7
 rectfill(0,0,127,127,c)
 fillp()
 
 local si=flr(1.5+0.7*cos(t*8)+0.7*sin(t*11))
 spr(si*5*16+1,8,90,14,5)
-- spr(slog*5*16+1,8,90,14,5)

 if message then
  local x=64+8*cos(t*2)
  str="!! ★ happy ★ !!"
  print(str,x-#str*2-2,13,0)
  print(str,x-#str*2-2,12,1)
  local x=64+8*cos(t*2+0.1)
  str="!!! ♥ holidays ♥ !!!"
  print(str,x-#str*2-2,21,0)
  print(str,x-#str*2-2,20,1)
 end
 
 foreach(sparks,draw_spark)
 foreach(balls,draw_ball)
 
-- circle(63,110,3.2)
 
 if btn(4,1) then
  print(stat(1),0,0,15)
 end
end



function update_ball(s)
 s.r=lerp(s.r,s.mr,0.1)
 
 s.y-=1
 s.mr-=0.09
 s.t-=0.01
 
 if s.mr<=1 or s.y<=s.r then
  del(balls,s)
 end
end

function update_spark(s)
 s.t+=0.01
 
 local n=#s.tail
 if n>3 then
  del(s.tail,s.tail[1])
 end
 
 s.y-=s.vy
 s.x=s.ox+s.mc*cos(s.ma*s.t+s.ka)

 add(s.tail,{x=s.x,y=s.y})

 if n>0 and s.tail[1].y<0 then
  del(sparks,s)
 end
end

function draw_ball(s)
 local x=s.x+s.mc*cos(s.t*s.ma+s.ka)
 
 circle(x,s.y,s.r)
end

function draw_spark(s)
 local n=#s.tail
 
 if n==0 then return end
 
 local lx,ly=s.x,s.y
 for i=n,1,-1 do
  local p=s.tail[i]
  line(lx,ly,p.x,p.y,min(10+i,15))
  lx,ly=p.x,p.y
 end
end

function create_ball(x,y)
 local s={
  x=x,
  y=y,
  r=0,
  mr=5+rnd(5),
  t=1+rnd(1),
  ka=rnd(1),
  ma=2+rnd(4),
  mc=2+rnd(6)
 }
 
 add(balls,s)
end

function create_spark(x,y)
 local s={
  x=x,
  y=y,
  ox=x,
  vy=2+rnd(3),
  tail={},
  t=0,
  ka=rnd(1),
  ma=2+rnd(2),
  mc=4+rnd(8)
 }
 
 add(sparks,s)
end


function circle(x,y,r)
 x,y,r=flr(x),flr(y),flr(r+.5)
 local y1,y2=y-r,y+r
 local dr=127/r
 for i=-r,r do
  local rx=max(flr(circle_data[flr(i*dr)]/dr+0.5),1)
  hline(x-rx,x+rx,y+i)
 end
end

function init_circle_draw()
 cls()
 circfill(127,127,127,1)
 circle_data={}

 for y=-128,-1 do
  for x=0,127 do
   if pget(128+y,x)==1 then
    circle_data[y]=127-x
    break
   end
  end
 end
 
 for y=0,127 do
  circle_data[y]=circle_data[-y-1]
 end
end

function init_log()
 local str="trasevol_dog"
 print(str,64-#str*2,112,1)

 for y=88,127 do
  for x=0,127 do
   local c=sget(x,y)
   
   if pget(x,y)==1 then
    if c==2 then
     sset(x,y,1)
     sset(x,y+1,4)
     c=1
    elseif c==4 then
     sset(x,y,1)
     sset(x,y+1,4)
     c=2
    end
   end
   
   if c>0 and rnd(16)<1 then
    if c==2 then
     sset(x,y,1)
    elseif c==4 then
     sset(x,y,2)
    end
   end
  end
 end

 for i=0,2 do
 
  for y=88,127 do
   for x=0,127 do
    local c=sget(x,y)
    local nc
    
    if c>0 then
     if c==1 then
      nc=2
     elseif c==2 then
      nc=1
     elseif c==4 then
      nc=0
     end
     
     local d=dist((x-64)/2,(y-96)*1.5)-2+i*0.5+rnd(2)
     local k=flr(8.5-d/4+1)
     if k==2 then
      k=3
     elseif k<2 then
      k=min(2-(k-3),7)
     end
     
     
     nc=mid(nc+k,0,8)
    else
     nc=15
    end
    
    sset(x,(y-88)+i*40,nc)
   end
  end
  
 end
end


function lerp(a,b,i) return (1-i)*a+i*b end
function sqr(a) return a*a end
function sqrdist(xa,ya,xb,yb) if xb then xa=xb-xa ya=yb-ya end return sqr(xa)+sqr(ya) end
function dist(xa,ya,xb,yb) return sqrt(sqrdist(xa,ya,xb,yb)) end
