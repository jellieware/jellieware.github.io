function hline(x1,x2,y)
-- local a1=bor(0x6000,y*64+flr(x1/8)*4)
-- local a2=bor(0x6000,y*64+flr(x2/8)*4)
-- 
-- local dx1=x1%8
-- local dx2=x2%8
-- 
-- if x2-x1<=8 then
--  return
-- end
-- 
-- if dx1>0 then
--  hlineleft(a1,dx1)
--  a1+=4
-- end
--
-- if dx2>0 then
----  hlineright(a2,8-dx2)
-- end

 local a1=bor(0x6000,y*64+flr(x1/2))
-- local a2=bor(0x6000,y*64+flr(x2/2))
 
 if x1%2==1 then
  hlineodd(a1)
  a1+=1
  x1+=1
 end
 
 local dx=x2-x1
 local rem=dx%8
 local i=(dx-rem)/2
 
 local a2=a1+i
 
 if rem>0 then
  hlineright[rem](a2)
  --ohlineright(a2,rem)
 end
-- print(a2)
 --hlineright(a2,rem)

 for a=a1,a2-1,4 do
  hlinemain(a)
 end
end

function hlineodd(a)
 local v=peek(a)
 if v<0xf0 then
  v+=0x10
 end
 poke(a,v)
end

function hlineleft(a,ofs)
 ofs=7

 local v=peek4(a)

 v=rotr(v,(7)*4)

 for i=ofs,7 do
  if band(v,0x.000f)<0x.000f then
   v+=0x.0001
  end
  v=rotl(v,4)
 end
 
 v=rotl(v,(9-ofs)*4)
 
 poke4(a,v)
end

function hlinemain(a)
 local v=peek4(a)
 
 --[[
 for i=0,7 do
  if band(v,0xf)<0xf then
   v+=1
  end
  v=rotl(v,4)
  --v=shl(v,4)
 end
 --]]
 
 if band(v,0xf)<0xf then
  v+=0x1
 end
 
 if band(v,0xf0)<0xf0 then
  v+=0x10
 end
 
 if band(v,0xf00)<0xf00 then
  v+=0x100
 end
 
 if band(v,0xf000)~=0xf000 then
  v+=0x1000
 end
 
 if band(v,0x.f)<0x.f then
  v+=0x.1
 end

 if band(v,0x.0f)<0x.0f then
  v+=0x.01
 end
 
 if band(v,0x.00f)<0x.00f then
  v+=0x.001
 end
 
 if band(v,0x.000f)<0x.000f then
  v+=0x.0001
 end
 
 poke4(a,v)
end

function ohlineright(a,ofs)
 local v=peek4(a)

 v=rotr(v,(ofs-1)*4)

 for i=1,ofs do
  if band(v,0x.000f)<0x.000f then
   v+=0x.0001
  end
  v=rotl(v,4)
 end
 
 v=rotr(v,4)
 
 poke4(a,v)
end

